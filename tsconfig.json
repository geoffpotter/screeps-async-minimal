{
  "compilerOptions": {
      "declaration": false,
      "target": "ES2015",
      //"module": "ES2015",
      "strict": true,
      "noUnusedLocals": true,
      "noUnusedParameters": true,
      "noImplicitReturns": true,
      "noFallthroughCasesInSwitch": true,
      "moduleResolution": "node",
      "outDir": "dist/",
      "rootDir": "src/"
  },
  "include": [
    "src/**/*",
  ],
  //"exclude": ["node_modules"]
}